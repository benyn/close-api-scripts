{
  "exclude": [
    "**/node_modules",
    "**/__pycache__",
    "src/experimental",
    "src/typestubs"
  ],
  "defineConstant": {
    "DEBUG": true
  },
  "venvPath": "venv",
  "reportMissingImports": true,
  "reportMissingTypeStubs": false,
  "reportArgumentType": false,
  "pythonVersion": "3.11",
  "pythonPlatform": "Darwin",
  "executionEnvironments": [
    {
      "root": "scripts"
    }
  ]
}
